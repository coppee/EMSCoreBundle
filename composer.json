{
	"name" : "elasticms/core-bundle",
	"description" : "The elasticms core bundle",
	"type" : "symfony-bundle",
	"authors" : [{
			"name" : "Mathieu De Keyzer",
			"email" : "mathieu@dekeyzer.net"
		},
		{
			"name" : "David Meert",
			"email" : "david.meert@smals.be"
		}
	],
	"keywords" : [
		"elasticms"
	],
	"license" : "MIT",
	"require" : {
		"php": "^7.2.0",
		"ext-iconv": "*",
		"ext-json": "*",
		"ext-soap": "*",
		"ext-openssl": "*",
		"sensio/framework-extra-bundle": "^5.1",
		"symfony/asset": "^4.1",
		"symfony/console": "^4.1",
		"symfony/expression-language": "^4.1",
		"symfony/form": "^4.1",
		"symfony/framework-bundle": "^4.1",
		"symfony/monolog-bundle": "^3.1",
		"symfony/orm-pack": "*",
		"symfony/process": "^4.1",
		"symfony/serializer-pack": "*",
		"symfony/swiftmailer-bundle": "^3.1",
		"symfony/twig-bundle": "^4.1",
		"symfony/validator": "^4.1",
		"symfony/web-link": "^4.1",
		"symfony/webpack-encore-pack": "*",
		"symfony/yaml": "^4.1",
		"elasticms/common-bundle": ">=1.6.21",
		"maennchen/zipstream-php" : "^0.5",
		"doctrine/doctrine-migrations-bundle" : "^1.3",
		"sensiolabs/ansi-to-html" : "^1.1",
		"guzzlehttp/guzzle" : "^6.3",
		"dompdf/dompdf": "^0.8.2",
		"caxy/php-htmldiff": "^0.1.7",
		"symfony/security": "^4.1",
		"symfony/translation": "^4.1",
		"psr/simple-cache": "^1.0"
	},
	"require-dev" : {
		"friendsofphp/php-cs-fixer" : "^2.11",
		"symfony/test-pack": "^1.0",
		"squizlabs/php_codesniffer": "3.*",
		"phpunit/phpunit" : "^5.7",
		"phpstan/phpstan": "^0.11.8"
	},
	"autoload" : {
		"psr-4" : {
			"EMS\\CoreBundle\\" : ""
		},
		"exclude-from-classmap" : [
			"tests/"
		]
	},
	"autoload-dev" : {
		"psr-4" : {
			"EMS\\CoreBundle\\Tests\\" : "tests/"
		}
	},
	"extra" : {
		"branch-alias" : {
			"dev-master" : "1.8.x-dev",
			"dev-demo" : "1.9.x-dev"
		}
	},
	"scripts": {
		"phpcs": "phpcs --standard=phpcs.xml.dist --extensions=php,lib,inc",
		"phpcbf": "phpcbf --standard=phpcs.xml.dist --extensions=php,lib,inc",
		"phpstan": "phpstan analyse ."
	}
}
